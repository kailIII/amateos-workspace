#--------------------------- Tool Specifications -------------------------
#
# Modify the following macros to reflect the tools you wish to use for
# compiling and linking your code.
#
CC = /Applications/MATLAB_R2008a/bin/mex
LD = $(CC)
 
MACHINE     = conmutador
TARGET      = sfun
CHART_SRCS 	= \
		c2_conmutador.c
MACHINE_SRC	= conmutador_sfun.c
MACHINE_REG = conmutador_sfun_registry.c
MEX_WRAPPER =
MAKEFILE    = conmutador_sfun.mku
MATLAB_ROOT	= /Applications/MATLAB_R2008a/toolbox/stateflow/stateflow/../../..
BUILDARGS   = 
#------------------------------ Include/Lib Path ------------------------------
 
USER_INCLUDES = -I/Users/EmP/Teleco/Cuarto/SCO/Practicas/P2/2x2/slprj/_sfprj/conmutador/_self/sfun/src -I/Users/EmP/Teleco/Cuarto/SCO/Practicas/P2/2x2 
MATLAB_INCLUDES = -I$(MATLAB_ROOT)/simulink/include \
						-I$(MATLAB_ROOT)/extern/include \
						-I/Applications/MATLAB_R2008a/stateflow/c/mex/include \
						-I/Applications/MATLAB_R2008a/stateflow/c/debugger/include

DSP_INCLUDES    = -I/Applications/MATLAB_R2008a/toolbox/eml/lib/dsp

INCLUDE_PATH = -I/Users/EmP/Teleco/Cuarto/SCO/Practicas/P2/2x2/slprj/_sfprj/conmutador/_self/sfun/src -I/Users/EmP/Teleco/Cuarto/SCO/Practicas/P2/2x2  $(MATLAB_INCLUDES) $(DSP_INCLUDES) $(COMPILER_INCLUDES)
 
#----------------- Compiler and Linker Options --------------------------------
 
# Optimization Options
OPT_OPTS = -O
 
# General User Options
OPTS =
 
CC_OPTS = $(OPT_OPTS) $(OPTS)
CPP_REQ_DEFINES = -DMATLAB_MEX_FILE
 
# Uncomment this line to move warning level to W4
# cflags = $(cflags:W3=W4)
CFLAGS = $(CC_OPTS) $(CPP_REQ_DEFINES) $(INCLUDE_PATH)
 
LDFLAGS = 
 
#----------------------------- Source Files -----------------------------------
 
REQ_SRCS  = $(MACHINE_SRC) $(MACHINE_REG) $(MEX_WRAPPER) $(CHART_SRCS)

USER_ABS_OBJS =

REQ_OBJS = $(REQ_SRCS:.cpp=.o)
REQ_OBJS2 = $(REQ_OBJS:.c=.o)
OBJS = $(REQ_OBJS2) $(USER_ABS_OBJS)
OBJLIST_FILE = conmutador_sfun.mol
SFCLIB = /Applications/MATLAB_R2008a/stateflow/c/mex/lib/maci/sfc_mex.a /Applications/MATLAB_R2008a/stateflow/c/debugger/lib/maci/sfc_debug.a /Applications/MATLAB_R2008a/toolbox/dspblks/lib/maci/dsp_sim_rt.a
USER_LIBS =
LINK_MACHINE_LIBS =
FIXEDPOINTLIB = -L/Applications/MATLAB_R2008a/bin/maci -lfixedpoint
UTLIB = -lut
  MAPCSF = /Applications/MATLAB_R2008a/tools/maci/mapcsf
   # RUN_MAPCSF_ON_UNIX is defined only if MAPCSF exists on this platform.
   ifneq ($(wildcard $(MAPCSF)),) # run MAPCSF if it exists on this platform
      RUN_MAPCSF_ON_UNIX =  /Applications/MATLAB_R2008a/tools/maci/mapcsf $@
   endif
 
#--------------------------------- Rules --------------------------------------
 
MEX_FILE_NAME = $(MACHINE)_$(TARGET).mexmaci
 
 $(MEX_FILE_NAME): $(MAKEFILE) $(OBJS) $(SFCLIB) $(USER_LIBS) $(MEXLIB)
	@echo ### Linking ...
	$(CC) -silent -output $(MEX_FILE_NAME) $(OBJS) $(USER_LIBS) $(LINK_MACHINE_LIBS) $(SFCLIB) $(FIXEDPOINTLIB) $(UTLIB)
	$(RUN_MAPCSF_ON_UNIX)

%.o :	%.c
	$(CC) -c $(CFLAGS) $<

%.o :	%.cpp
	$(CC) -c $(CFLAGS) $<

